local screenGui = Instance.new("ScreenGui")
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 200, 0, 300)
frame.Position = UDim2.new(0.5, -100, 0.5, -150)
frame.Parent = screenGui

local scripts = {
    {name = "Speed Boost", url = "https://raw.githubusercontent.com/man143252/karocia/refs/heads/main/Speed"},
    {name = "Jump Boost", url = "https://raw.githubusercontent.com/man143252/karocia/refs/heads/main/Jump"},
    {name = "Fly Mode", url = "https://raw.githubusercontent.com/man143252/karocia/refs/heads/main/Fly"}
}

for i, script in ipairs(scripts) do
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0, 180, 0, 50)
    button.Position = UDim2.new(0, 10, 0, 10 + (i-1)*60)
    button.Text = script.name
    button.Parent = frame

    button.MouseButton1Click:Connect(function()
        local success, err = pcall(function()
            local code = game:HttpGet(script.url)
            loadstring(code)()
        end)
        if not success then
            warn("Failed to execute script: " .. err)
        end
    end)
end

screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
